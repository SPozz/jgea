findall(St,start(St,1),Starts),random_member(S,Starts),retract(start(S,1)),assert(start(S,0)),gensym(nod,N1),gensym(nod,N2),assert(node_id(N1)),assert(node_id(N2)),assert(start(N1,1)),assert(start(N2,0)),assert(type(N1,operator)),findall(OV, operator_val(OV),OpValues),random_member(OpVal,OpValues),assert(value(N1,OpVal)),(maybe ->  assert(type(N2,input)),    n_input(InpMax),    random(0, InpMax, InpVal),                   assert(value(N2,InpVal));assert(type(N2,constant)),    random(0.001,2.0,ConstVal),	assert(value(N2,ConstVal)) ),gensym(edg,E1),gensym(edg,E2),assert(edge_id(E1)),assert(edge_id(E2)),assert(edge(S,N1,E1)),assert(edge(N2,N1,E2)).